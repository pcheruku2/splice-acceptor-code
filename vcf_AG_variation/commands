# example scripts

perl subset_trimer_vcf.pl uniq_trimers_AG_gain_loss_neutral.txt ../../tmp/trimer_sites_chr21_-_Intronic.bed ../../clinvar/clinvar.vcf > chr21_-_intronic_trimer_vcf_AG_variation.tsv
perl subset_trimer_vcf.pl uniq_trimers_AG_gain_loss_neutral.txt ../../tmp/trimer_sites_chr21_+_Intronic.bed ../../clinvar/clinvar.vcf > chr21_+_intronic_trimer_vcf_AG_variation.tsv
perl subset_trimer_vcf.pl uniq_trimers_AG_gain_loss_neutral.txt ../../tmp/trimer_sites_chr21_-_Exonic.bed ../../clinvar/clinvar.vcf > chr21_-_exonic_trimer_vcf_AG_variation.tsv
perl subset_trimer_vcf.pl uniq_trimers_AG_gain_loss_neutral.txt ../../tmp/trimer_sites_chr21_+_Exonic.bed ../../clinvar/clinvar.vcf > chr21_+_exonic_trimer_vcf_AG_variation.tsv


 awk '{print $10 "\t" $11 "\t" $12 "\t" $13 "\t" $14 "\t" $15 "\t" $16 "\t" $17 "\t" $1 "\t" $2 "\t" $3 "\t" $4 "\t" $5 "\t" $6 "\t" $7 "\t" $8 "\t" $9 }' chr21_+_intronic_trimer_vcf_AG_variation.tsv > tmp.vcf 


